<?xml version="1.0" encoding="UTF-8"?>

<div xmlns="http://www.w3.org/1999/xhtml" component="$model/UI2/system/components/justep/window/window" class="window" sysParam="false" data-bind="component:{name:'$model/UI2/system/components/justep/window/window'}" __justepbasexid__="__baseID__" components="$model/UI2/system/components/justep/barcode/barcodeImage,$model/UI2/system/components/justep/model/model,$model/UI2/system/components/justep/window/window," css="">  
  <div component="$model/UI2/system/components/justep/model/model" xid="model" style="display:none" data-bind="component:{name:'$model/UI2/system/components/justep/model/model'}" data-events="onLoad:modelLoad"/>  
  <div xid="div8" style="padding-left: 8px;"> 
    <label xid="label1" class="x-label">当前检查到的IP是：</label>  
    <select xid="ip" bind-change="ipChange" style="vertical-align:top;margin-top:8px;margin-right:4px"/>
    <label xid="label3" class="x-label">如果IP地址不是无线网络IP，请从左边选择正确IP。</label> 
  </div>
  <div xid="div6" style="padding-left: 8px;"> 
    <label xid="label4" class="x-label">小提示：手机/平板连接的无线网络需和电脑是同一局域网。</label> 
  </div>
  <div xid="div7" style="padding: 8px"> 
    <div xid="androidDiv" style="display:inline-block;">
      <img src="$model/UI2/system/deploy/package/img/android.png" alt="" xid="androidImage" style="cursor:pointer;padding-right: 16px" bind-click="androidImageClick"/>
      <div component="$model/UI2/system/components/justep/barcode/barcodeImage" class="xui-barcodeImage" type="qr" templateName="templateName" xid="androidQR" style="height:180px;width:180px;" barcodeConfig="{mw:1.0}" config="{&quot;barcodeConfig&quot;:&quot;{mw:1.0}&quot;,&quot;type&quot;:&quot;qr&quot;}" data-bind="component:{name:'$model/UI2/system/components/justep/barcode/barcodeImage'}">
        <img id="img-androidQR" src="about:blank" style="vertical-align:middle;"/>
      </div>
    </div>  
    <div xid="iosDiv" style="display:inline-block;">
      <img src="$model/UI2/system/deploy/package/img/apple.png" alt="" xid="iosImage" style="cursor:pointer;padding-right: 16px" bind-click="iosImageClick"/>
      <div component="$model/UI2/system/components/justep/barcode/barcodeImage" class="xui-barcodeImage" type="qr" templateName="templateName" xid="iosQR" style="height:180px;width:180px;" barcodeConfig="{mw:1.0}" config="{&quot;barcodeConfig&quot;:&quot;{mw:1.0}&quot;,&quot;type&quot;:&quot;qr&quot;}" data-bind="component:{name:'$model/UI2/system/components/justep/barcode/barcodeImage'}">
        <img id="img-iosQR" src="about:blank" style="vertical-align:middle;"/>
      </div>
    </div>  
    <label xid="tipLabel" class="x-label" style="font-size:x-large;font-weight:bold;display:none">未生成android或者ios本地App</label>
  </div>
  <div xid="div1" style="padding-left: 8px;">
    <label xid="howInstallLabel" class="x-label" bind-click="howInstallLabelClick" style="cursor:pointer;">不明白怎么安装？别急，点这里</label> 
  </div>
</div>
